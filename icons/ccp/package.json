{
  "name": "@icongo/ccp",
  "description": "Credit Card & Payment Icons",
  "version": "1.0.3",
  "homepage": "http://icongo.github.io/#/icons/ccp",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/icongo.git"
  },
  "scripts": {
    "cpy": "node cpy.mjs",
    "svgo": "npm run cpy && npm run svgo1 && npm run svgo2",
    "svgo1": "svgo -f .cache_svg -o svg",
    "svgo2": "svgo -f .cache_svg -o ../../www/public/icon/ccp",

    "icongo": "icongo --prefix CCP --source svg -o ./src",
    "build": "npm run svgo && npm run icongo && tsbb build",
    "start": "npm run build"
  },
  "keywords": [
    "icongo",
    "ccp",
    "react-icons",
    "icons",
    "icon"
  ],
  "author": "jaywcjlove",
  "license": "MIT"
}
