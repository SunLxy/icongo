{
  "name": "@icongo/ci",
  "description": "CoreUI Icons",
  "version": "1.0.3",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "homepage": "http://icongo.github.io/#/icons/ci",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/icongo.git"
  },
  "scripts": {
    "cpy": "node ./cpy.mjs && npm run svgo",
    "svgo": "npm run svgo1 && npm run svgo2",
    "svgo1": "svgo -f .cache_svg -o svg",
    "svgo2": "svgo -f .cache_svg -o ../../www/public/icon/ci",

    "tsbb": "tsbb build",
    "icongo": "icongo --prefix CI --source svg -o ./src",
    "build": "npm run cpy && npm run icongo && npm run tsbb",
    "start": "npm run build"
  },
  "keywords": [
    "icongo",
    "coreui",
    "react-icons",
    "icons",
    "icon"
  ],
  "author": "jaywcjlove",
  "license": "MIT"
}
