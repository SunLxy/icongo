{
  "name": "@icongo/fc",
  "description": "Flat Color Icons",
  "version": "1.0.3",
  "author": "jaywcjlove",
  "license": "MIT",
  "homepage": "http://icongo.github.io/#/icons/fc",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/icongo.git"
  },
  "scripts": {
    "svgo": "npm run svgo1 && npm run svgo2",
    "svgo1": "svgo -f data/svg -o svg",
    "svgo2": "svgo -f data/svg -o ../../www/public/icon/fc",

    "icongo": "icongo --prefix FC --source svg -o ./src",
    "build": "npm run svgo && npm run icongo && tsbb build --no-source-maps",
    "start": "npm run build"
  },
  "keywords": [
    "icongo",
    "fc",
    "Flat",
    "Color",
    "react-icons",
    "icons",
    "icon"
  ]
}
