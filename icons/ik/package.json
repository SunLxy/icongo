{
  "name": "@icongo/ik",
  "version": "1.0.3",
  "author": "jaywcjlove",
  "description": "Ikonate Icons",
  "license": "MIT",
  "homepage": "http://icongo.github.io/#/icons/ik",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/icongo.git"
  },
  "scripts": {
    "svgo": "npm run svgo1 && npm run svgo2",
    "svgo1": "svgo -f data/icons -o svg",
    "svgo2": "svgo -f data/icons -o ../../www/public/icon/ik",

    "tsbb": "tsbb build",
    "icongo": "icongo --prefix IK --source svg -o ./src",
    "build": "npm run svgo && npm run icongo && tsbb build",
    "start": "npm run build"
  },
  "keywords": [
    "icongo",
    "Ikonate",
    "ik",
    "react-icons",
    "icons",
    "icon"
  ]
}
