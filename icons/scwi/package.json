{
  "name": "@icongo/scwi",
  "version": "1.0.3",
  "description": "Spectrum Workflow Icons",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "homepage": "http://icongo.github.io/#/icons/scwi",
  "scripts": {
    "cpy": "cpy 'data/dist/spectrum-css-ccx-workflow-icons/dist/22/*.svg' .cache_svg --flat",
    "svgo": "npm run cpy && npm run svgo1 && npm run svgo2",
    "svgo1": "svgo -f .cache_svg -o svg",
    "svgo2": "svgo -f .cache_svg -o ../../www/public/icon/scwi",

    "icongo": "icongo --prefix SCWI --source svg -o ./src",
    "build": "npm run svgo && npm run icongo && tsbb build --no-source-maps",
    "start": "npm run build"
  },
  "keywords": [
    "icongo",
    "Spectrum-CSS",
    "workflow-icons",
    "workflow",
    "react-icons",
    "icons",
    "icon"
  ],
  "author": "jaywcjlove",
  "license": "MIT"
}
