{
  "name": "@icongo/sti",
  "description": "Super Tiny Icons",
  "version": "1.0.3",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "homepage": "http://icongo.github.io/#/icons/supertinyicons",
  "scripts": {
    "svgo": "npm run svgo1 && npm run svgo2",
    "svgo1": "svgo -f data/images/svg -o svg",
    "svgo2": "svgo -f data/images/svg -o ../../www/public/icon/sti",
    "icongo": "icongo --prefix STI --filter='(calendar).svg' --source svg -o ./src",
    "build": "npm run svgo && npm run icongo && tsbb build",
    "start": "npm run build"
  },
  "keywords": [
    "icongo",
    "supertinyicons",
    "react-icons",
    "icons",
    "icon"
  ],
  "author": "jaywcjlove",
  "license": "MIT"
}
